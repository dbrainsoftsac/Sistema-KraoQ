<div class="page-header">
  <%= link_to usuarios_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_usuario_path(@usuario), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show usuario</h1>
</div>

<dl class="dl-horizontal">
  <dt>No usuario:</dt>
  <dd><%= @usuario.no_usuario %></dd>

  <dt>Tipousuario:</dt>
  <dd><%= @usuario.TipoUsuario %></dd>

  <dt>No apellido paterno:</dt>
  <dd><%= @usuario.no_apellido_paterno %></dd>

  <dt>No apellido materno:</dt>
  <dd><%= @usuario.no_apellido_materno %></dd>

  <dt>No dni:</dt>
  <dd><%= @usuario.no_dni %></dd>

  <dt>Email:</dt>
  <dd><%= @usuario.email %></dd>

</dl>
