<div class="page-header">
  <%= link_to comentarios_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_comentario_path(@comentario), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show comentario</h1>
</div>

<dl class="dl-horizontal">
  <dt>Local:</dt>
  <dd><%= @comentario.Local %></dd>

  <dt>Sala:</dt>
  <dd><%= @comentario.Sala %></dd>

  <dt>Usuario:</dt>
  <dd><%= @comentario.Usuario %></dd>

  <dt>Tx comentario:</dt>
  <dd><%= @comentario.tx_comentario %></dd>

  <dt>Qt estrellas:</dt>
  <dd><%= @comentario.qt_estrellas %></dd>

</dl>
